{
	"name": "discord-bots",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"check:case": "case-police --ignore '**/coverage/**'",
		"check:case:fix": "npm run check:case --fix",
		"check:types": "tsc",
		"eslint": "eslint '**/*.{json,mjs,ts}'",
		"eslint:fix": "npm run eslint -- --fix",
		"generate:types": "source .env && npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public >| ./types/supabase.ts",
		"netlify": "netlify",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@js-temporal/polyfill": "0.4.4",
		"@netlify/functions": "1.6.0",
		"@sentry/node": "7.53.1",
		"@supabase/supabase-js": "2.23.0",
		"@types/node": "18.16.16",
		"colord": "2.9.3",
		"date-fns": "2.30.0",
		"discord-api-types": "0.37.42",
		"html-entities": "2.3.3",
		"node-html-parser": "6.1.5",
		"phin": "3.7.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.3",
		"@commitlint/config-conventional": "17.6.3",
		"@vitest/coverage-c8": "0.31.1",
		"@zeebats/eslint-config": "1.3.1",
		"case-police": "0.6.1",
		"netlify-cli": "14.4.0",
		"simple-git-hooks": "2.8.1",
		"typescript": "5.0.4",
		"vitest": "0.31.1"
	}
}
