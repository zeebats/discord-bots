{
	"name": "discord-bots",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"check:case": "case-police --ignore '**/coverage/**'",
		"check:case:fix": "npm run check:case --fix",
		"check:types": "tsc",
		"eslint": "eslint '**/*.{json,mjs,ts}'",
		"eslint:fix": "npm run eslint -- --fix",
		"netlify": "netlify",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@js-temporal/polyfill": "0.4.3",
		"@netlify/functions": "1.4.0",
		"@sentry/node": "7.38.0",
		"@supabase/supabase-js": "2.8.0",
		"@types/node": "18.14.0",
		"cross-fetch": "3.1.5",
		"date-fns": "2.29.3",
		"discord-api-types": "0.37.35",
		"esbuild": "0.17.10",
		"html-entities": "2.3.3",
		"isomorphic-unfetch": "4.0.2",
		"netlify-plugin-discord": "0.0.2",
		"node-html-parser": "6.1.5"
	},
	"devDependencies": {
		"@commitlint/cli": "17.4.4",
		"@commitlint/config-conventional": "17.4.4",
		"@vitest/coverage-c8": "^0.28.4",
		"@zeebats/eslint-config": "1.3.1",
		"case-police": "0.5.11",
		"eslint-plugin-jsonc": "2.6.0",
		"netlify-cli": "13.0.1",
		"simple-git-hooks": "2.8.1",
		"typescript": "4.9.5",
		"vitest": "0.28.5"
	}
}
