{
	"name": "discord-bots",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:types": "source .env && npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public >| ./types/supabase.ts",
		"lint:scripts:fix": "pnpm run eslint --fix",
		"lint:scripts": "eslint '**/*.{json,mjs,ts}'",
		"lint:types:typescript": "tsc --noEmit",
		"prepare": "simple-git-hooks",
		"netlify": "netlify",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@js-temporal/polyfill": "0.4.4",
		"@netlify/functions": "2.0.2",
		"@sentry/node": "7.69.0",
		"@supabase/supabase-js": "2.33.2",
		"@types/node": "20.6.2",
		"colord": "2.9.3",
		"date-fns": "2.30.0",
		"discord-api-types": "0.37.56",
		"html-entities": "2.4.0",
		"node-html-parser": "6.1.10",
		"phin": "3.7.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-conventional": "17.7.0",
		"@vitest/coverage-c8": "0.33.0",
		"@zeebats/eslint-config": "1.4.0-beta.7",
		"lint-staged": "14.0.1",
		"netlify-cli": "16.3.4",
		"simple-git-hooks": "2.9.0",
		"typescript": "5.2.2",
		"vitest": "0.34.4"
	}
}
