		"@types/node": "20.11.24",
		"colord": "2.9.3",
		"date-fns": "3.3.1",
		"discord-api-types": "0.37.71",
		"html-entities": "2.5.2",
		"node-html-parser": "6.1.12",
		"phin": "3.7.0",
		"unstorage": "1.10.1",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "19.0.3",
		"@commitlint/config-conventional": "19.0.3",
		"@vitest/coverage-c8": "0.33.0",
		"@zeebats/eslint-config": "1.4.0-beta-9",
		"lint-staged": "15.2.2",
		"netlify-cli": "17.17.2",
		"simple-git-hooks": "2.10.0",
		"typescript": "5.3.3",
		"vitest": "1.3.1"
	}
