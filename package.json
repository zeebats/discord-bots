		"@sentry/node": "7.83.0",
		"@types/node": "20.10.0",
		"colord": "2.9.3",
		"date-fns": "3.2.0",
		"discord-api-types": "0.37.65",
		"html-entities": "2.4.0",
		"node-html-parser": "6.1.11",
		"phin": "3.7.0",
		"unstorage": "1.10.1",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@commitlint/cli": "18.4.3",
		"@commitlint/config-conventional": "18.4.3",
		"@vitest/coverage-c8": "0.33.0",
		"@zeebats/eslint-config": "1.4.0-beta-8",
		"lint-staged": "15.1.0",
		"netlify-cli": "17.8.0",
		"simple-git-hooks": "2.9.0",
		"typescript": "5.3.2",
		"vitest": "0.34.6"
	}
}
